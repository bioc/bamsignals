citHeader("To cite bamsignals in publications use:")

citEntry(entry = "Article",
		title = "Bamsignals: Extract read count signals from bam files",
		author = personList(as.person(c( "Alessandro Mammana", as.person("Johannes Helmuth")),
		journal = "Bioinformatics",
		year = "2015",
		volume = "",
		number = "",
		pages = "",
		url = "",
		textVersion =
		paste("Alessandro Mammana, Johannes Helmuth (2015).",
			"Bamsignals: Extract read count signals from bam files.",
			"Bioinformatics, , .",
			"URL .")
)
